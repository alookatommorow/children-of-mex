<div class="page-wrapper">
  <div class="well gallery">
    <h1>
      <div class="horizontal divider">Gallery</div>
    </h1>
    <div class="gallery-menu" data-content-container=".gallery-content">
      <a class="item active" data-gallery-link="photo">Photos</a>
      <a class="item" data-gallery-link="video">Videos</a>
      <a class="item" data-gallery-link="press">Press</a>
    </div>
    <div class="gallery-content">
      <%= render partial: "pages/photos" %> <!-- refactor? -->
      <%= render partial: "pages/videos" %>
      <%= render partial: "pages/press" %>
    </div>
  </div>
</div>